{
  "input": {
    "name": "Input",
    "input_type": "word", 
    "description": "Player must input a valid command within the time limit.",
    "valid_responses": [
      "dodge",
      "duck",
      "jump",
      "leap",
      "roll",
      "sidestep",
      "back",
      "catch"
    ],
    "default_duration": 3.0,
    "score_on_success": 10,
    "success_message_default": "You successfully dodged the danger!",
    "failure_message_default": "You failed to react in time!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "button_mash": {
    "name": "Button Mash",
    "input_type": "mash",
    "description": "Player must press a key or button repeatedly to meet a target count.",
    "default_duration": 5.0,
    "target_mash_count_default": 20,
    "score_on_success": 15,
    "success_message_default": "You were fast enough!",
    "failure_message_default": "Not fast enough!",
    "hp_damage_on_failure_default": 5,
    "is_fatal_on_failure_default": false
  },
  "spam_any_key": {
    "name": "Break Free / Spam",
    "input_type": "mash",
    "description": "Player must mash any key to build a score to a threshold.",
    "default_duration": 3.0,
    "target_score_default": 25,
    "success_message_default": "You break loose!",
    "failure_message_default": "You’re still stuck!",
    "hp_damage_on_failure_default": 5,
    "is_fatal_on_failure_default": false
  },
  "sequence_input": {
    "name": "Sequence Input",
    "input_type": "sequence",
    "description": "Player must type a specific word or code exactly as shown.",
    "default_duration": 7.0,
    "score_on_success": 20,
    "success_message_default": "Sequence input correct!",
    "failure_message_default": "Incorrect sequence or too slow!",
    "hp_damage_on_failure_default": 5,
    "is_fatal_on_failure_default": false
  },
  "hold_and_release": {
    "name": "Hold and Release",
    "input_type": "hold_release",
    "description": "Tap and hold anywhere on the screen, then release within the time window.",
    "default_duration": 4.0,
    "release_window_default": [0.6, 0.8],
    "success_message_default": "Perfect timing!",
    "failure_message_default": "Mistimed release!",
    "hp_damage_on_failure_default": 8,
    "is_fatal_on_failure_default": false
  },
  "balance_meter": {
    "name": "Balance Meter",
    "input_type": "alternate",
    "default_duration": 3.0,
    "success_message_default": "You keep your balance!",
    "failure_message_default": "You slip and fall!",
    "keys_default": ["a", "d"],
    "target_alternations_default": 12
  },
    "reaction_single_key": {
    "name": "Reaction Check",
    "input_type": "single_key",
    "description": "Player must press a specific key that appears on screen within a short time window.",
    "default_duration": 2.0,
    "window_tolerance": 0.25,
    "success_message_default": "You react in time!",
    "failure_message_default": "Too slow!",
    "hp_damage_on_failure_default": 5,
    "is_fatal_on_failure_default": false
  },
  "timed_choice": {
    "name": "Timed Choice",
    "input_type": "choice",
    "description": "Player must choose from several options before time runs out.",
    "choices_default": ["left", "right", "forward"],
    "default_duration": 5.0,
    "success_message_default": "Good call!",
    "failure_message_default": "Wrong move!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "hold_to_threshold": {
    "name": "Hold to Threshold",
    "input_type": "hold",
    "description": "Player must hold a key for a specific duration to reach a threshold.",
    "default_duration": 4.0,
    "required_hold_time_default": 2.5,
    "success_message_default": "You held on long enough!",
    "failure_message_default": "You let go too soon!",
    "hp_damage_on_failure_default": 8,
    "is_fatal_on_failure_default": false
  },
  "timed_release_window": {
    "name": "Timed Release Window",
    "input_type": "hold_release",
    "description": "Player must release a held key within a specific time window.",
    "default_duration": 4.0,
    "release_window_default": [0.6, 0.8],
    "success_message_default": "Perfect timing!",
    "failure_message_default": "Mistimed release!",
    "hp_damage_on_failure_default": 8,
    "is_fatal_on_failure_default": false
  },
  "alternating_keys": {
    "name": "Struggle / Alternate",
    "input_type": "alternate",
    "description": "Player must alternate between two keys rapidly.",
    "keys_default": ["a", "d"],
    "target_alternations_default": 12,
    "default_duration": 5.0,
    "success_message_default": "You broke free!",
    "failure_message_default": "You couldn’t build enough momentum!",
    "hp_damage_on_failure_default": 5,
    "is_fatal_on_failure_default": false
  },
  "pattern_memory": {
    "name": "Pattern Memory",
    "input_type": "pattern",
    "description": "Player must repeat a sequence of inputs shown to them.",
    "default_duration": 8.0,
    "pattern_length_default": 4,
    "alphabet_default": ["up","down","left","right"],
    "success_message_default": "Pattern accepted!",
    "failure_message_default": "Pattern error!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "rhythm_timing": {
    "name": "Rhythm Timing",
    "input_type": "rhythm",
    "description": "Tap in rhythm with the beat!",
    "default_duration": 6.0,
    "target_beats": 5,
    "beat_interval": 0.8,
    "timing_window": 0.25,
    "required_accuracy_default": 0.8,
    "success_message_default": "You kept the rhythm!",
    "failure_message_default": "You missed the beat!",
    "hp_damage_on_failure_default": 7,
    "is_fatal_on_failure_default": false
  },
  "aim_and_click": {
    "name": "Aim and Click",
    "input_type": "aim_click",
    "description": "Player must aim and click on a moving target within the time limit.",
    "default_duration": 4.0,
    "target_zone_size_default": 0.2,
    "success_message_default": "Direct hit!",
    "failure_message_default": "You missed!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "drag_track": {
    "name": "Drag Track",
    "input_type": "drag",
    "description": "Player must drag a cursor along a track without leaving the bounds.",
    "default_duration": 5.0,
    "required_accuracy_default": 0.85,
    "success_message_default": "You stayed on track!",
    "failure_message_default": "You veered off!",
    "hp_damage_on_failure_default": 8,
    "is_fatal_on_failure_default": false
  },
  "directional_input": {
    "name": "Directional Input",
    "input_type": "sequence",
    "description": "Player must input a sequence of directions correctly.",
    "default_duration": 5.0,
    "directions_default": ["up", "down", "left", "right"],
    "sequence_length_default": 3,
    "success_message_default": "You navigated successfully!",
    "failure_message_default": "Wrong direction!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "multiword_code": {
    "name": "Multiword Code",
    "input_type": "code",
    "description": "Player must type a multiword code exactly as shown.",
    "default_duration": 7.0,
    "code_length_default": 2,
    "success_message_default": "Code accepted!",
    "failure_message_default": "Incorrect code!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "precision_tap_count": {
    "name": "Precision Tap Count",
    "input_type": "tap",
    "description": "Player must tap a key an exact number of times within the time limit.",
    "default_duration": 4.0,
    "required_tap_count_default": 10,
    "success_message_default": "Perfect tap count!",
    "failure_message_default": "Too many or too few taps!",
    "hp_damage_on_failure_default": 7,
    "is_fatal_on_failure_default": false
  },
  "mouse_spiral": {
    "name": "Mouse Spiral",
    "input_type": "spiral",
    "description": "Player must move the mouse in a spiral pattern to succeed.",
    "default_duration": 5.0,
    "required_spiral_accuracy_default": 0.8,
    "success_message_default": "Spiral complete!",
    "failure_message_default": "Spiral failed!",
    "hp_damage_on_failure_default": 10,
    "is_fatal_on_failure_default": false
  },
  "targeted_cancel": {
    "name": "Targeted Cancel",
    "input_type": "cancel",
    "description": "Player must select the one correct option from a list of duds.",
    "default_duration": 5.0,
    "choices_default": ["red","blue","green","yellow"],
    "correct_choice_default": "blue",
    "success_message_default": "Correct choice!",
    "failure_message_default": "Wrong choice!",
    "hp_damage_on_failure_default": 999,
    "is_fatal_on_failure_default": true
  }
}